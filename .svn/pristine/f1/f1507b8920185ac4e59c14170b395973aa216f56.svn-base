                        "        \"FSaleOrgId\": {\n" +
                        "            \"FNumber\": \""+fNumberSaleOrg+"\"\n" +
                        "        },\n" +
                        "        \"FCustomerID\": {\n" +
                        "            \"FNumber\": \""+fNumberCustomer+"\"\n" +
                        "        },\n" +
                        "        \"FStockOrgId\": {\n" +
                        "            \"FNumber\": \""+fNumberStockOrg+"\"\n" +
                        "        },\n" +
                        "        \"FReceiverID\": {\n" +
                        "            \"FNumber\": \""+fNumberCustomer+"\"\n" +
                        "        },\n" +
