    @Override
    public ResponseResult saveMarketReturn(String acctId,String dataCentreUserName,String dataCentrePassword,String fNumberSaleOrg,String fDate,String fNumberStockOrg,String fNumberFrecust,
                           String fNumberBillType,String fNumberSettleOrg,String fNumberSettleCurr,String entityJson,String fID ,String fBillNo) {
        Boolean login = K3cloudConfig.login(acctId,dataCentreUserName,dataCentrePassword);
        if(login){
            try {
                //必填项：
                //基本信息：FBillHead
                //   销售组织：FSaleOrgId  (必填项)
                //	 日期：FDate  (必填项)
                //	 库存组织：FStockOrgId  (必填项)
                //	 退货客户：FRetcustId  (必填项)
                //	 单据类型：FBillTypeID  (必填项)
                //明细信息：FEntity
                //	 物料编码：FMaterialId  (必填项)
                //	 库存单位：FUnitID  (必填项)
                //	 货主：FOwnerId  (必填项)
                //	 退货类型：FReturnType  (必填项)
                //	 退货日期：FDeliveryDate  (必填项)
                //财务信息：SubHeadEntity
                //   结算组织：FSettleOrgId  (必填项)
                //	 结算币别：FSettleCurrId  (必填项)
                MarketReturnModel model = new MarketReturnModel();
                FSaleOrgId fSaleOrgId = new FSaleOrgId();
                fSaleOrgId.setFNumber(fNumberSaleOrg);
                model.setFSaleOrgId(fSaleOrgId);
                model.setFDate(fDate);
                FStockOrgId fStockOrgId = new FStockOrgId();
                fSaleOrgId.setFNumber(fNumberStockOrg);
                model.setFStockOrgId(fStockOrgId);
                FRetcustId fretcustid = new FRetcustId();
                fretcustid.setFNumber(fNumberFrecust);
                model.setFRetcustId(fretcustid);
                FBillTypeID fBillTypeID = new FBillTypeID();
                fBillTypeID.setFNUMBER(fNumberBillType);
                model.setFBillTypeID(fBillTypeID);
                JSONArray array = JSON.parseArray(entityJson);
                List<FEntity> fentityList = new ArrayList<>();
                for (int i = 0; i < array.size(); i++) {
                    String fMaterialId = array.getJSONObject(i).getString("FMaterialId");
                    String fUnitID = array.getJSONObject(i).getString("FUnitID");
                    String fOwnerId = array.getJSONObject(i).getString("FOwnerId");
                    String fReturnType = array.getJSONObject(i).getString("FReturnType");
                    String fDeliveryDate = array.getJSONObject(i).getString("FDeliveryDate");
                    FEntity fentity = new FEntity();
                    FMaterialId materialId = new FMaterialId("CH4438");
                    materialId.setFNumber(fMaterialId);
                    fentity.setFMaterialId(materialId);
                    FUnitID unitID = new FUnitID();
                    unitID.setFNumber(fUnitID);
                    fentity.setFUnitID(unitID);
                    FOwnerId ownerId = new FOwnerId();
                    ownerId.setFNumber(fOwnerId);
                    fentity.setFOwnerId(ownerId);
                    FReturnType returntype = new FReturnType();
                    returntype.setFNumber(fReturnType);
                    fentity.setFReturnType(returntype);
                    fentity.setFDeliveryDate(fDeliveryDate);
                    fentityList.add(fentity);
                }
                model.setFEntity(fentityList);
                Subheadentity subheadentity = new Subheadentity();
                FSettleOrgID settleOrgID = new FSettleOrgID();
                settleOrgID.setFNumber(fNumberSettleOrg);
                subheadentity.setFSettleOrgId(settleOrgID);
                FSettleCurrID settleCurrID = new FSettleCurrID();
                settleCurrID.setFNumber(fNumberSettleCurr);
                subheadentity.setFSettleCurrId(settleCurrID);
                model.setSubHeadEntity(subheadentity);
                if(!StringUtils.isBlank(fID)){
                    model.setFID(Integer.valueOf(fID));
                }
                if(!StringUtils.isBlank(fBillNo)){
                    model.setFBillNo(fBillNo);
                }
                MarketReturnSaveParam marketReturnSaveParam = new MarketReturnSaveParam();
                marketReturnSaveParam.setModel(model);
                String s =JSON.toJSONString(marketReturnSaveParam,SerializerFeature.WriteMapNullValue, SerializerFeature.WriteNullStringAsEmpty);
                JSONObject save = InvokeHelper.Save("SAL_RETURNSTOCK", s);
                return ResponseResult.success(save);
            } catch (Exception e) {
                e.printStackTrace();
                return ResponseResult.success(e);
            }
        }
        return ResponseResult.success(K3cloudConfig.LOGIN_FAIL);
    }

