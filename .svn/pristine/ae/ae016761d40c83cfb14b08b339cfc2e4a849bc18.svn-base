package com.lnmj.common.Enum;

import com.lnmj.common.Enum.base.BaseEnum;

/**
 * @Author: yilihua
 * @Date: 2019/9/6 16:14
 * @Description: 出库业务类型
 */
public enum OutStorageBusinessTypeEnum implements BaseEnum {

    //业务类型（其他出库：物料领用、资产领用、WMI业务、费用物料领用）
    STANDARD_OTHER(11, "物料领用"),
    CAPITAL(12, "资产领用"),
    WMI_OTHER(13, "WMI业务"),
    COST_ITEM(14, "费用物料领用"),
    ;

    private Integer code;
    private String desc;

    OutStorageBusinessTypeEnum(Integer code, String desc) {
        this.code = code;
        this.desc = desc;
    }

    public Integer getCode() {
        return code;
    }

    public void setCode(Integer code) {
        this.code = code;
    }

    public String getDesc() {
        return desc;
    }

    public void setDesc(String desc) {
        this.desc = desc;
    }}
