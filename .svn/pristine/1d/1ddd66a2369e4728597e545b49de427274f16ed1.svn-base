            </if>
            <if test="status != null">
                Status = #{status},
            </if>
            <if test="modifyOperator != null">
                ModifyOperator = #{modifyOperator},
            </if>
            DataChange_LastTime = now()
        </set>
        where StoreCategoryId = #{storeCategoryId}
    </update>

