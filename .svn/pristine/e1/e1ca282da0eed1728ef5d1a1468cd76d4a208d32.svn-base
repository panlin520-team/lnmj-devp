  redis:
    host: 222.211.86.214
    port: 6379
    password: panlin123456
    database: 5
    timeout: 10000
    charset: GBK
  datasource:
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.jdbc.Driver
    druid:
#      first:
      url: jdbc:mysql://222.211.86.214:1086/lnmj_k3cloud?useUnicode=true&characterEncoding=utf8&serverTimezone=UTC&allowMultiQueries=true
      username: root
      password: lnmj123456
#    second:
#      url: jdbc:mysql://222.211.86.214:1086/lnmj_account?useUnicode=true&characterEncoding=utf8&serverTimezone=UTC&allowMultiQueries=true
#      username: root
#      password: lnmj123456
      # 下面为连接池的补充设置，应用到上面所有数据源中
      initialSize: 5
      minIdle: 5
      maxActive: 500
      # 配置获取连接等待超时的时间
      maxWait: 6000
      # 配置间隔多久才进行一次检测，检测需要关闭的空闲连接，单位是毫秒
      timeBetweenEvictionRunsMillis: 60000
      # 配置一个连接在池中最小生存的时间，单位是毫秒
      minEvictableIdleTimeMillis: 300000
      validationQuery: SELECT 1 FROM DUAL
      testWhileIdle: true
      testOnBorrow: false
      testOnReturn: false
      # 配置监控统计拦截的filters，去掉后监控界面sql无法统计，'wall'用于防火墙
      filters: stat,wall,log4j
      filter:
        stat:
          logSlowSql: true
      #开启游标检查  mysql不适用
      poolPreparedStatements: false
      #poolPreparedStatements 为true 生效
      maxPoolPreparedStatementPerConnectionSize: 20
#mybatis配置
mybatis:
  mapper-locations: classpath:mapper/*/*.xml
  # mybatis的model所在的包
  type-aliases-package: com.lnmj.core.bean,com.lnmj.k3cloud.entity
  #开启驼峰转换
  configuration:
    map-underscore-to-camel-case: true
    #log-impl: org.apache.ibatis.logging.stdout.StdOutImpl
    #打印mybatiesSql语句
pagehelper:
  helperDialect: mysql
  reasonable: true
  supportMethodsArguments: true
  params: count=countSql
  page-size-zero: true
logging:
  config: classpath:log4j2.yml
AOP_POINTCUT_EXPRESSION: 'execution(* *..*.service..*.*(..))'
